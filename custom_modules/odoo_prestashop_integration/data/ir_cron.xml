<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">

        <record forcecreate="True" id="update_stock_in_prestashop" model="ir.cron">
            <field name="name">Update Stock From Odoo To Prestashop</field>
            <field name="model_id" ref="model_prestashop_product_detail"/>
            <field name="state">code</field>
            <field name="code">model.auto_export_stock_from_odoo_to_prestashop()</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>

        <record forcecreate="True" id="update_sale_in_odoo" model="ir.cron">
            <field name="name">Update Sale From Prestashop To Odoo</field>
            <field name="model_id" ref="model_sale_order"/>
            <field name="state">code</field>
            <field name="code">model.auto_export_sale_from_prestashop_to_odoo()</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">30</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>

    </data>
</odoo>