<odoo>

    <record id="delivery_stock_picking_withcarrier_form_view_inherit_custom" model="ir.ui.view">
        <field name="name">delivery.stock.picking_withcarrier.form.view.inherit.custom</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="delivery.view_picking_withcarrier_out_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group/field[@name='delivery_type']" position="after">
                <field name="x_carrier_tracking_ref"/>
            </xpath>
                        <xpath expr="//group/label[1]" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//group/div[@name='tracking']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
        </field>
    </record>

</odoo>